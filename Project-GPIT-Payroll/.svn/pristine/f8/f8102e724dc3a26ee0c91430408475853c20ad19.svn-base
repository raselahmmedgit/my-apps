        </th>
        <th>
            @Html.DisplayNameFor(model => model.double_rate_hour)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.triple_rate_hour)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.four_times_rate_hour)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.total_ot)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.holiday)
