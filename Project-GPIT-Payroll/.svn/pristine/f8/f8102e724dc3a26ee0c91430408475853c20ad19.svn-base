            @Html.DisplayFor(modelItem => item.normal_days)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.total_days)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.evening_shift)
        </td>
