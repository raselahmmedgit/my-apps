        <th>
            @Html.DisplayNameFor(model => model.approve_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.one_way)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.two_way)
        </th>
       
    </tr>

@foreach (var item in Model) {
