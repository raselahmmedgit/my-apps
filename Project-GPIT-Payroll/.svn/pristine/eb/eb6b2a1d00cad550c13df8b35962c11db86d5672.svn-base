using System.Web.Mvc;

namespace PayrollWeb.Utility
{
    public class ModelClientValidationInputBoxBinaryChoice : ModelClientValidationRule
    {
        public ModelClientValidationInputBoxBinaryChoice(string errorMessage, object other)
        {
            ErrorMessage = errorMessage;
            ValidationType = "inputboxbinarychoice";
            ValidationParameters["other"] = other;
        }
    }
}