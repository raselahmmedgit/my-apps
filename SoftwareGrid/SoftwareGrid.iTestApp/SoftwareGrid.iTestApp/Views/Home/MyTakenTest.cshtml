
@{
    ViewBag.Title = "My Taken Test";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <div class="data-content">
        <div class="col-md-12">
            <div class="portlet-box">
                <div class="portlet-header">
                    <h3><span id="TestTitle">My Taken Test</span> (<span id="TotalCount"></span>)</h3>
                </div>
                <div class="portlet-body">
                    <div class="row">
                        <input type="hidden" name="ShowPerPageItem" id="ShowPerPageItem" value="15" />
                        <div class="table-responsive" id="div-test"></div>
                        <div class="paging-content"></div>
                    </div>
                    <div class="row" style="display: none" id="no-data-content">
                        <div class="col-md-12">
                            <h3 style="margin-top: -15px">No Data found.</h3>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script type="text/vash" id="tmpTest">
    @@model.data.forEach(function(v){
    <div class="col-md-4">
        <div class="test-item">
            <center><img src="@@v.TestIconPath"></center>
            <span>@@v.TestName</span>
            <div class="test-action">
                <a href="javascript:;" class="add-to-test">@@v.Score %</a>
                <a href="/Home/TestResult?takenId=@@v.TakenId" target="_blank">View Details</a>
            </div>
        </div>
    </div>
    })
</script>

<script src="~/assets/admin/pages/otherscripts/MyTakenTest.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        MyTakenTest.init();
    });
</script>

<style>
    .paging-content {
        margin-left: 12px;
    }

    .pagination {
        display: block;
        margin: 0px;
        padding: 0px;
    }
</style>
